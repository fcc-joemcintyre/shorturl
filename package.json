{
  "name": "shorturl",
  "author": "Joe McIntyre (j_h_mcintyre@yahoo.com)",
  "description": "URL Shortener service",
  "version": "0.3.2",
  "type": "module",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcc-joemcintyre/shorturl"
  },
  "license": "MIT",
  "dependencies": {
    "express": "4.18.1"
  },
  "scripts": {
    "build": "rm -rf lib && swc src -d lib",
    "test": "mocha && npm run postman:run",
    "postman:run": "cd postman && node --loader ts-node/esm test-api.ts",
    "postman:server": "cd postman && node --loader ts-node/esm test-api.ts --server",
    "start": "nodemon lib/main.js --port=3000",
    "lint": "eslint --ext .js,.ts,.tsx --no-error-on-unmatched-pattern .",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@cygns/eslint-config-node": "6.1.0",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.2.203",
    "@types/express": "4.17.13",
    "@types/mocha": "9.1.1",
    "@types/newman": "5.3.0",
    "@types/node": "17.0.44",
    "@typescript-eslint/parser": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "eslint": "8.17.0",
    "eslint-plugin-import": "2.26.0",
    "mocha": "10.0.0",
    "newman": "5.3.2",
    "nodemon": "2.0.16",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "engines": {
    "node": "^18.3"
  }
}
